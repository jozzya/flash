{
    "name": "jozzya/flash",
    "type": "drupal-profile",
    "description": "Flash distribution profile",
    "license": "apache-2.0",
    "require": {
        "ext-dom": "*",
        "cweagans/composer-patches": "~1.0",
        "drupal/core": "^10.3",
        "drupal/access_unpublished": "^1.2",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advagg": "*",
        "drupal/bamboo_twig": "*",
        "drupal/chosen": "^3.0",
        "drupal/config_filter": "^2.2",
        "drupal/config_ignore": "^2.3",
        "drupal/config_pages": "^2.9",
        "drupal/config_rewrite": "^1.4",
        "drupal/config_split": "^2.0@beta",
        "drupal/content_lock": "^2.2",
        "drupal/core-project-message": "^10.3",
        "drupal/core-recommended": "^10.3",
        "drupal/core-composer-scaffold": "^10.3",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_group": "^3.1",
        "drupal/google_tag": "^1.4",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/metatag": "^1.16",
        "drupal/multiline_config": "^1.0",
        "drupal/paragraphs": "^1.12",
        "drupal/password_policy": "*",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/replicate": "^1.0",
        "drupal/scheduler": "^1.3",
        "drupal/webform": "^6.0",
        "jozzya/flash_theme": "^1.0",
        "vlucas/phpdotenv": "^5.1"
    },
    "require-dev": {
        "composer/installers": "^1.10",
        "drupal/core-dev": "*",
        "drupal/devel": "*",
        "drush/drush": "*",
        "phpspec/prophecy-phpunit": "^2",
        "weitzman/drupal-test-traits": "*"
    },
    "config": {
        "preferred-install": {
            "drupal/core": "dist",
            "jozzya/flash_*": "source"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "php-http/discovery": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-8.x-4.x": "4.x-dev"
        },
        "composer-exit-on-patch-failure": "true",
        "drupal-scaffold": {
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/.gitattributes": false,
                "[web-root]/.csslintrc": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/robots.txt": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/web.config": false
            },
            "locations": {
                "web-root": "web/"
            }
        },
        "drush": {
            "services": {
                "drush.services.yml": "^9 || ^10"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        },
        "patches-ignore": {
        }
    },
    "repositories": {
        "whoops": {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/whoops-3185130"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
